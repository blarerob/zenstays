{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "./dist",
    "rootDir": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./app/*"]
    },
    "types": [
      "node",
      "react",
      "react-dom",
      "@types/lodash",
      "@types/react-vis",
      "@types/uuid"
    ],
    "target": "ES2017"
    },
    "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
      "dist/index.js"
    ],
    "exclude": [
    "node_modules",
    "dist",
    ".next",
    "out"
    ]
}